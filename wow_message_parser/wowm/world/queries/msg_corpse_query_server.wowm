#tag_all versions "1.12";

enum CorpseQueryResult : u8 {
    NOT_FOUND = 0;
    FOUND = 1;
}

smsg MSG_CORPSE_QUERY_Server = 0x0216 {
    CorpseQueryResult result;
    if (result == FOUND) {
        Map map;
        f32 position_x;
        f32 position_y;
        f32 position_z;
        Map corpse_map;
    }
}

