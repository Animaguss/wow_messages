#tag_all versions "1.12";

enum PlayerChatTag : u8 {
    NONE = 0;
    AFK = 1;
    DND = 2;
    GM = 3;
}

cmsg CMSG_MESSAGECHAT = 0x0095 {
    (u32)ChatType chat_type;   
    Language language;
    if (chat_type == WHISPER) {
        CString target_player;
    } else if (chat_type == CHANNEL) {
        CString channel;
    }
    CString message;
}

