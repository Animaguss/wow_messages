#tag_all versions "1.12";

enum FriendStatus : u8 {
    OFFLINE = 0;
    ONLINE = 1;
    AFK = 2;
    UNKNOWN3 = 3;
    DND = 4;
}

struct Friend {
    u64 guid;
    FriendStatus status;
    if (status != OFFLINE) {
        Area area;
        u32 level;
        (u32)Class class;
    }
}

smsg SMSG_FRIEND_LIST = 0x0067 {
    u8 amount_of_friends;
    Friend[amount_of_friends] friends;
}

