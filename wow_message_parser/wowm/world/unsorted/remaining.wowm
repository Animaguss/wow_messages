     || chat_type == GUILD 
     || chat_type == OFFICER 
     || chat_type == RAID 
     || chat_type == RAID_LEADER 
     || chat_type == RAID_WARNING 
     || chat_type == BATTLEGROUND 
     || chat_type == BATTLEGROUND_LEADER
